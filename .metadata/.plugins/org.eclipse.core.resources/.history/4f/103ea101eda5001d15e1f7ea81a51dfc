package NumbersPrograms;

public class Swap2Numbers {

	public static void main(String[] args) {
		
		int a = 50;
		int b = 30;
		
//		int c ;
//		
//		c= a;
//		a=b;
//		b=c;
//		
//		System.out.println(a);
		System.out.println(b);
		
		a = a+b;
		b = a-b;
		a =a-b;
		System.out.println(a);
		System.out.println(b);
		
	}

}
